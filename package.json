{
  "name": "setlist-organizer",
  "version": "1.0.0",
  "description": "Set List Organizer for REDLINE",
  "main": "main.js",
  "author": "Lorenzo Moralez",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win portable"
  },
  "build": {
    "appId": "com.redline.setlistorganizer",
    "productName": "SetListOrganizer",
    "win": {
      "target": "portable",
      "icon": "assets/icon.ico"
    },
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    }
  },
  "devDependencies": {
    "electron": "30.0.9",
    "electron-builder": "^26.0.12"
  }
}
